		{
			"name": "Company Info",
			"item": [
				{
					"name": "Get Company Info",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Status code is 200\", function () {\r",
									"    pm.response.to.have.status(200);\r",
									"});\r",
									"\r",
									"\r",
									"const response = pm.response.json();\r",
									"\r",
									"\r",
									"pm.test(\"Headquarters should be Rocket Road\", () => {\r",
									"    pm.expect(response.headquarters.address).to.eql(\"Rocket Road\")\r",
									"});\r",
									"\r",
									"\r",
									"pm.test(\"City should be Hawthorne\", () => {\r",
									"    pm.expect(response.headquarters.city).to.eql(\"Hawthorne\")\r",
									"});\r",
									"\r",
									"pm.test(\"State should be California\", () => {\r",
									"    pm.expect(response.headquarters.state).to.eql(\"California\")\r",
									"});\r",
									"\r",
									"pm.test(\"There should be links with a website\", () => {\r",
									"    pm.expect(response.links.website).to.eql(\"https://www.spacex.com/\")\r",
									"});\r",
									"\r",
									"pm.test(\"There should be links with a flickr\", () => {\r",
									"    pm.expect(response.links.flickr).to.eql(\"https://www.flickr.com/photos/spacex/\")\r",
									"});\r",
									"\r",
									"pm.test(\"There should be links with a twitter\", () => {\r",
									"    pm.expect(response.links.twitter).to.eql(\"https://twitter.com/SpaceX\")\r",
									"});\r",
									"\r",
									"pm.test(\"There should be links with a elon_twitter\", () => {\r",
									"    pm.expect(response.links.elon_twitter).to.eql(\"https://twitter.com/elonmusk\")\r",
									"});\r",
									"\r",
									"pm.test(\"Name should be SpaceX\", () => {\r",
									"    pm.expect(response.name).to.eql(\"SpaceX\")\r",
									"});\r",
									"\r",
									"\r",
									"pm.test(\"Founder should be Elon Musk\", () => {\r",
									"    pm.expect(response.founder).to.eql(\"Elon Musk\")\r",
									"});\r",
									"\r",
									"pm.test(\"Founded should be 2002\", () => {\r",
									"    pm.expect(response.founded).to.eql(2002)\r",
									"});\r",
									"\r",
									"pm.test(\"employees should be 9500\", () => {\r",
									"    pm.expect(response.employees).to.eql(9500)\r",
									"});\r",
									"\r",
									"pm.test(\"vehicles should be 4\", () => {\r",
									"    pm.expect(response.vehicles).to.eql(4)\r",
									"});\r",
									"\r",
									"pm.test(\"launch_sites should be 3\", () => {\r",
									"    pm.expect(response.launch_sites).to.eql(3)\r",
									"});\r",
									"\r",
									"pm.test(\"test_sites should be 3\", () => {\r",
									"    pm.expect(response.test_sites).to.eql(3)\r",
									"});\r",
									"\r",
									"pm.test(\"ceo should be Elon Musk\", () => {\r",
									"    pm.expect(response.ceo).to.eql(\"Elon Musk\")\r",
									"});\r",
									"\r",
									"pm.test(\"cto should be Elon Musk\", () => {\r",
									"    pm.expect(response.cto).to.eql(\"Elon Musk\")\r",
									"});\r",
									"\r",
									"pm.test(\"coo should be Gwynne Shotwell\", () => {\r",
									"    pm.expect(response.coo).to.eql(\"Gwynne Shotwell\")\r",
									"});\r",
									"\r",
									"pm.test(\"cto_propulsion should be Tom Mueller\", () => {\r",
									"    pm.expect(response.cto_propulsion).to.eql(\"Tom Mueller\")\r",
									"});\r",
									"\r",
									"pm.test(\"There should be links with a summary\", () => {\r",
									"    pm.expect(response.summary).to.eql(\"SpaceX designs, manufactures and launches advanced rockets and spacecraft. The company was founded in 2002 to revolutionize space technology, with the ultimate goal of enabling people to live on other planets.\")\r",
									"});\r",
									"\r",
									"\r",
									"\r",
									"pm.test(\"There should be links with a id\", () => {\r",
									"    pm.expect(response.id).to.eql(\"5eb75edc42fea42237d7f3ed\")\r",
									"});\r",
									"pm.test(\"There should be links with a valuation\", () => {\r",
									"    pm.expect(response.valuation).to.eql(74000000000)\r",
									"});"
								],
								"type": "text/javascript"
							}
